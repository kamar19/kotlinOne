<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/fmlBgMoveList"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/bg"
        app:layout_constraintBottom_toTopOf="@+id/fmlGuidelinePath10"
        app:layout_constraintTop_toTopOf="@+id/fmlGuidelinePath10"
        tools:ignore="MissingConstraints" />


    <View
        android:id="@+id/fmlMainBackGround"
        android:layout_width="375dp"
        android:layout_height="812dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/bg_botton"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fmlGuidelinePath10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fmlGuidelinePath20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.18" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fmlGuidelinePath35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.36" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fmlguidelineCenter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.23" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fmlguidelineCenter2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />



    <View
        android:id="@+id/fmlCombinedShape"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="16dp"
        android:background="@drawable/combined_shape"
        app:layout_constraintBottom_toTopOf="@+id/fmlGuidelinePath10"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/fmlGuidelinePath10"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/fmlWarszawaPo"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="8dp"
        android:gravity="center_vertical"
        android:lineSpacingExtra="4sp"
        android:text="@string/fmlWarszawaPo"
        android:textAppearance="@style/fmlStyleWarszawaPo"
        app:layout_constraintBottom_toTopOf="@+id/fmlGuidelinePath10"
        app:layout_constraintStart_toEndOf="@+id/fmlCombinedShape"
        app:layout_constraintTop_toTopOf="@+id/fmlGuidelinePath10"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/backGroundMovie"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="12dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/fml_bg_move"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="fmlImageViewMovie"
        app:layout_constraintEnd_toStartOf="@+id/fmlguidelineCenter2"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginBottom="48dp"

        app:layout_constraintTop_toBottomOf="@+id/fmlWarszawaPo"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/fmlImageViewMovie"
        android:layout_width="0dp"
        android:layout_height="0dp"

        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"

        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="6dp"
        android:background="@drawable/movie_movies_list"
        android:contentDescription="@string/todo"

        app:layout_constraintEnd_toEndOf="@+id/backGroundMovie"
        app:layout_constraintStart_toStartOf="@+id/backGroundMovie"
        app:layout_constraintTop_toTopOf="@+id/backGroundMovie"
        app:layout_constraintBottom_toBottomOf="@id/backGroundMovie"
        tools:ignore="MissingConstraints"


        app:contentDescription="1:1"










        />


    <View
        android:id="@+id/fmlRectangle"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="6dp"
        android:alpha="0.9"
        android:background="@drawable/fml_rectangle"
        app:layout_constraintBottom_toTopOf="@+id/fmlGuidelinePath20"
        app:layout_constraintStart_toStartOf="@+id/fmlImageViewMovie"
        app:layout_constraintTop_toTopOf="@+id/fmlGuidelinePath20"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/fmlSomeId"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/fmlSomeId"
        android:textAppearance="@style/fmlStyleSomeId"
        app:layout_constraintBottom_toBottomOf="@+id/fmlRectangle"
        app:layout_constraintEnd_toEndOf="@+id/fmlRectangle"
        app:layout_constraintStart_toStartOf="@+id/fmlRectangle"
        app:layout_constraintTop_toTopOf="@+id/fmlRectangle"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/like"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="6dp"
        android:alpha="0.75"
        android:background="@drawable/like"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@+id/fmlGuidelinePath20"
        app:layout_constraintEnd_toEndOf="@+id/fmlImageViewMovie"
        app:layout_constraintTop_toTopOf="@+id/fmlGuidelinePath20"
        tools:ignore="MissingConstraints"
        android:contentDescription="@string/todo" />

    <TextView
        android:id="@+id/fmlTag"
        android:layout_width="146dp"
        android:layout_height="12dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:text="@string/fmlTag"
        android:textAppearance="@style/tag"
        android:lineSpacingExtra="3sp"
        android:gravity="center_vertical"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/fmlTextViewMin"
        android:layout_width="154dp"
        android:layout_height="12dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:text="@string/fmlTextViewMin"
        android:textAppearance="@style/fmlStyleTextViewMin"
        android:lineSpacingExtra="3sp"
        android:gravity="center_vertical"
        tools:ignore="MissingConstraints"
        app:layout_constraintBottom_toBottomOf="@id/fmlImageViewMovie"
        app:layout_constraintStart_toStartOf="@id/fmlImageViewMovie"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="8dp"
        />




</androidx.constraintlayout.widget.ConstraintLayout>